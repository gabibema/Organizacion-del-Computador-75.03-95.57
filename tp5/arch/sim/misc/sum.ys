		irmovq ele1, %rdi
		call sum_list           # sum_list(ele1)
		ret

sum_list:
		irmovq $0, %rax         
		jmp test                
loop:
		mrmovq 0(%rdi), %r10    
		addq %r10, %rax         
		mrmovq 8(%rdi), %rdi    # escribe la direcci√≥n del siguiente nodo en %rdi
		
test:
		andq %rdi, %rdi         
		jne loop                # para cuando es 0
